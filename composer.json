{
  "name": "buena/sdk",
  "description": "Official PHP SDK for Buena.ai API v2 - LinkedIn automation and lead management",
  "version": "1.0.0",
  "keywords": [
    "buena",
    "linkedin",
    "automation",
    "lead-management",
    "api",
    "sdk",
    "php"
  ],
  "homepage": "https://docs.buena.ai",
  "license": "MIT",
  "authors": [
    {
      "name": "Buena.ai",
      "email": "support@buena.ai",
      "homepage": "https://docs.buena.ai"
    }
  ],
  "support": {
    "source": "https://github.com/buena-ai/sdks",
    "issues": "https://github.com/buena-ai/sdks/issues"
  },
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "friendsofphp/php-cs-fixer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Buena\\SDK\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Buena\\SDK\\Test\\": "test/"
    }
  }
}
